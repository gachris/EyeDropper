<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:devtoolbox="http://devtoolbox.wpf/winfx/2021/xaml/presentation">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/DevToolbox.Wpf;component/Themes/Styles/Button/Generic.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource ButtonStyle}" TargetType="Button">
        <Setter Property="MinWidth" Value="80" />
        <Setter Property="Padding" Value="6" />
        <Setter Property="devtoolbox:UIElementHelper.CornerRadius" Value="2" />
    </Style>

</ResourceDictionary>
